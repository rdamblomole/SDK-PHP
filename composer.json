{
    "name": "rdamblomole/php-sdk",
    "description": "SDK para TodoPago",
    "version": "1.10.15",
    "require": {
        "php": ">=5.3",
        "magento/framework": "*"
    },
    "autoload": {
        "psr-4": {
            "TodoPago\\": "TodoPago/lib",
            "TodoPago\\Test\\": "TodoPago/test"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rdamblomole/SDK-PHP.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "php-mock/php-mock-phpunit": "^1.1"
    }
}

